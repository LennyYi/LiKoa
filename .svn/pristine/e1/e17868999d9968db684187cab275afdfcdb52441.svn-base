.table-ui-state-hover { border: 1px solid #79b7e7; background: #d0e5f5 url(../images/ui-bg_glass_75_d0e5f5_1x400.png) 50% 50% repeat-x;  }
.table-ui-state-hover a, .ui-state-hover a:hover { color: #1d5987; text-decoration: none; }
.table-ui-state-active { border: 1px solid #F1F1F1; background: #f5f8f9 url(../images/ui-bg_inset-hard_100_f5f8f9_1x100.png) 50% 50% repeat-x;  }
.table-ui-state-active a, .ui-state-active a:link, .ui-state-active a:visited { color: #e17009; text-decoration: none; }
.table-ui-state-highlight  {border: 1px solid #F1F1F1; background: #fbec88 url(../images/ui-bg_flat_55_fbec88_40x100.png) 50% 50% repeat-x;  }

