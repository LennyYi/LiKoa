/* CSS Document */
body {
	font-size: 12px;
	color: #333333;
	margin: 1px 1px 1px 1px;
	background: #fff repeat-x bottom left fixed;
}

a:link, a:visited {
	color: #CC4200;
	text-decoration: none;
}

a:hover, a:active {
	color: #FF0000;
	text-decoration: none;
}

tr {
	font: normal normal 12px Arial, Verdana, "黑体";
	text-align: left;
}

input, select, textarea, .input{
	font-size: 12px;
	font-family: Arial;
}

input, .input {border: 1px solid #c9c9c9;}

.eflow_title {
	font: normal bold 25px Arial;
	color: #ffffff;
}

.eflow_subtitle {
	font-size: 18px;
	font-family: Arial;
	color: #ffffff;
}

.btn3_mouseout {
	border: #4F4F4F 1px solid;
	padding: 2px 2px 2px 0;
	font-size: 12px;
	font-weight: bold;
	FILTER: progid: DXImageTransform.Microsoft.Gradient(GradientType=0, StartColorStr = #ffffff, EndColorStr = #DAD0B3)
	CURSOR: hand;
	COLOR: black;
}

.btn3_mouseout {
	border: #b7b7b7 1px solid;
	padding: 2px 2px 2px 0px;
	font-size: 13px;
	font-weight: bold;
	FILTER: progid: DXImageTransform.Microsoft.Gradient(GradientType=0, StartColorStr = #ffffff, EndColorStr = #ededed );
	CURSOR: hand;
	COLOR: #606060;
	margin-right: 5px;
}

.btn3_mouseover, .btn3_mousedown {
	border: #D4D4D4 1px solid;
	padding: 2px 2px 2px 0px;
	font-size: 13px;
	FILTER: progid: DXImageTransform.Microsoft.Gradient(GradientType=0, StartColorStr = #7B7B7B, EndColorStr = #7B7B7B );
	CURSOR: hand;
	COLOR: white;
	font-weight: bold;
	margin-right: 5px;
	background-color: #868686;
}
.btn3_mouseup {
	border: #b7b7b7 1px solid;
	padding: 2px 2px 2px 0px;
	font-size: 13px;
	FILTER: progid: DXImageTransform.Microsoft.Gradient(GradientType=0, StartColorStr = #ffffff, EndColorStr = #ededed );
	BORDER-LEFT: #b7b7b7 1px solid;
	CURSOR: hand;
	COLOR: #606060;
	margin-right: 5px;
	background: #fff;
}

