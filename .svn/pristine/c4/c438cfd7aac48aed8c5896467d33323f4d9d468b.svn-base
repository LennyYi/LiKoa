#DHTMLgoodies_formTooltipDiv{
	color:#FFF;
	font-family:arial;
	font-weight:bold;
	font-size:0.9em;
	line-height:120%;
}
.DHTMLgoodies_formTooltip_closeMessage{
	color:#FFF;
	font-weight:normal;
	font-size:0.8em;
}

a:link.DHTMLgoodies_formTooltip_closeMessage {
	color: #FFF;
	text-decoration: none;
}

a:visited.DHTMLgoodies_formTooltip_closeMessage {
	text-decoration: none;
	color: #FFF;
}
a:hover.DHTMLgoodies_formTooltip_closeMessage {
	text-decoration: none;
	color: #FFF;
}
a:active.DHTMLgoodies_formTooltip_closeMessage {
	text-decoration: none;
	color: #FFF;
}