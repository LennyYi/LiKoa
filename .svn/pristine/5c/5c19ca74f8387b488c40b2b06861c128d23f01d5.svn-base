body {
	text-align: center; 
} 

#searchMainDiv{
	margin-top: 10px;
	width:96%;
}

#searchResultDetailDiv{
	margin-bottom:80px;
}

.levelBar2{
	height:35px;
	line-height:35px;
	text-align:left;
	font-weight:bold;
	text-indent:20px;
	color:#666666;
	margin-top:2px;
}


.levelBar {
	height: 50px; 
	color: #ffffff; 
	font-size: 15px; 
	line-height:50px;
	background-color: rgb(211, 17, 69);
	font-weight:bold;
}



.searchResultList{
	width:100%;
	border-collapse:collapse;
	border: 1px solid #cdcdcd;
	margin-bottom:10px;
	background:#F8F8F8;
}

.searchResultList tr{
	height:30px;
}

.searchResultList td{
	text-indent:10px;
	border: 1px solid #cdcdcd;
	width:25%;
}

.searchResultList a{
	color:#000000;
}


.searchResultFilter{
	border: 1px solid #cdcdcd;
	float:left;
	margin-bottom:-1px;
	width:100%;
	background:#F8F8F8;
}

.searchResultFilter tr{
	height:30px;
}

.searchResultFilter td.rightspace{
	width:60%;
}

.searchResultFilter td.rightspace2{
	width:80%;
}

.searchResultFilter td{
	text-indent:5px;
}

#searchOption{
	margin-top:20px;
	margin-bottom:20px;
}

tr.searchframe:hover td,tr.searchframe td{
	background:#D3D3D4;
}

.baseInfo div{
	height: 35px; 
	line-height:35px;
	color: #ffffff; 
	font-size: 15px; 
	background-color: rgb(118, 118, 118);
	text-align: left;
	font-weight:bold;
	text-indent:30px;
}

.baseInfo .tableLabel{
	font-weight:bold;
	color:#666666;
	width:150px;
}


.baseInfo td{
	text-indent:5px;
}

#search_result_policy_basic,#search_result_bill,#search_result_claim{
	margin-top:30px;
}

#searchKey{
	height:25px;
	line-height:25px;
	width:250px;
	text-indent:5px;
}

#searchBtn{
	height:25px;
	width:100px;
	background:#D5063C;
	color:#ffffff;
	cursor:pointer;
}


.SearchTypeP{
	background:#D3D3D4;
	width:60px;
	text-align:center;
	cursor:pointer;
}

.SearchTypeM{
	background:#ffffff;
	width:60px;
	colspan:3;
	text-align:center;
	cursor:pointer;
}


#searchOption table{
	 border-collapse:collapse;
	 colpadding:0;
}