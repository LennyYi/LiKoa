function formatDate1(date,format1,format2) {
	if (!date) return;   
    if (!format2) format2 = "yyyy-MM-dd";
    
    var forms;
    if(format1 == "yyyyMMdd")
    	forms = ["yyyy","MM","dd"];
    else
    	forms = format1.replace(/-/g, "/").split("/");
    
    var dates;
    
    if(format1 == "yyyyMMdd"){	
    	dates = date.match(/(\d{4})(\d{2})(\d{2})/);
    	dates = [dates[1],dates[2],dates[3]];
    }
    else
    	dates = date.replace(/-/g, "/").split("/")
    		
    var yy;
    var MM;
    var dd;
    var l = forms.length;
    for(var i=0;i<l;i++){
		if(forms[i]=="yyyy"){
			yy=dates[i];
		}
		if(forms[i]=="MM"){
			MM=parseInt(dates[i],10)-1;	//when the number is begin with 0,will return 0, 20150312 Justin Bin fix bug
		}
		if(forms[i]=="dd"){
			dd=dates[i];
		}
	}
    switch(typeof date) {   
        case "string":	
            date = new Date(yy,MM,dd);   
            break;   
        case "number":   
            date = new Date(date);   
            break;   
    } 
    if (!date instanceof Date) return;  
    var dict = {   
        "yyyy": date.getFullYear(),   
        "M": date.getMonth()+1,   
        "d": date.getDate(),   
        "H": date.getHours(),   
        "m": date.getMinutes(),   
        "s": date.getSeconds(),   
        "MM": ("" + (date.getMonth() + 101)).substr(1),   
        "dd": ("" + (date.getDate() + 100)).substr(1),   
        "HH": ("" + (date.getHours() + 100)).substr(1),   
        "mm": ("" + (date.getMinutes() + 100)).substr(1),   
        "ss": ("" + (date.getSeconds() + 100)).substr(1)   
    };       
    return format2.replace(/(yyyy|MM?|dd?|HH?|ss?|mm?)/g, function() {   
        return dict[arguments[0]];   
    });                   
}   



