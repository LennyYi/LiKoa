				jQuery(selectedID).append("<option value='"+item.NOVA_PRODCODE+"'>"+item.NOVA_PRODCODE_LOCAL+"</option>");   
			});
		}
		
		if(selectedVal!='')
			jQuery(selectedID).val(selectedVal);
		else
			jQuery(selectedID).find("option:first-child").attr("selected", true);
	}
}

