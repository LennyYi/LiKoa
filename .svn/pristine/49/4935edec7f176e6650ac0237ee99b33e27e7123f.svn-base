			jQuery(selectedID).empty();
			//NOVA-66-20150309 Hinson BEGIN
//				jQuery(selectedID).append("<option value='null'></option>");
			jQuery(selectedID).append("<option value=''></option>");
			//NOVA-66-20150309 Hinson END
