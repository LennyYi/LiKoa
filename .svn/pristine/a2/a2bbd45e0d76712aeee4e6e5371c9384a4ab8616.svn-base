$(function(){
	var px =window.screen.height/2;
	var py =window.screen.width/2;
	$( "#dialog" ).dialog({
		autoOpen: false,
		width: 200,
		height:160,
		position: [px,py],
		buttons: [
			{
				css:{ "color": "#000000", "background": "gray" },
				text: "Ok",
				click: function() {
					$( this ).dialog( "close" );
				}
			},
			{
				text: "Cancel",
				css:{ "color": "#000000", "background": "gray" },
				click: function() {
					$( this ).dialog( "close" );
				}
			}
		]
	});
	
});
jQuery(document).ready(function(){
