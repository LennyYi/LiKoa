package com.aiait.eflow.housekeeping.vo;

import java.util.Collection;

import com.aiait.framework.vo.BaseVO;
/*
 * 这个vo与表不是太对应，基本可以用，无妨
 */
public class SystemFieldVO extends BaseVO {
	private String fieldId;

	private String fieldLabel;

	private String fieldType;

	private int columnType;

	private int columnLength;
	
	private String srcSQL;
	
	private String params;
	
	private Collection optionList;

	public int getColumnLength() {
		return columnLength;
	}

	public void setColumnLength(int columnLength) {
		this.columnLength = columnLength;
	}

	public int getColumnType() {
		return columnType;
	}

	public void setColumnType(int columnType) {
		this.columnType = columnType;
	}

	public String getFieldId() {
		return fieldId;
	}

	public void setFieldId(String fieldId) {
		this.fieldId = fieldId;
	}

	public String getFieldLabel() {
		return fieldLabel;
	}

	public void setFieldLabel(String fieldLabel) {
		this.fieldLabel = fieldLabel;
	}

	public String getFieldType() {
		return fieldType;
	}

	public void setFieldType(String fieldType) {
		this.fieldType = fieldType;
	}

	public Collection getOptionList() {
		return optionList;
	}

	public void setOptionList(Collection optionList) {
		this.optionList = optionList;
	}

	public String getSrcSQL() {
		return srcSQL;
	}

	public void setSrcSQL(String srcSQL) {
		this.srcSQL = srcSQL;
	}

	public String getParams() {
		return params;
	}

	public void setParams(String params) {
		this.params = params;
	}



}
