package com.aiait.eflow.housekeeping.vo;

import java.util.ArrayList;
import java.util.Collection;

import com.aiait.framework.vo.BaseVO;

public class StaffVO extends BaseVO {
	private String staffCode;

	private String staffName;

	private String logonId;

	private String status;

	private String teamCode;

	private String teamName;
	
	private String email;
	
	private String currentRoleId;
	
	private String userType;
	
	private String chineseName;
	
	private String bankDetail;
	
	private String accountNo;
	
	private double additionalAmount;
	
	private String displayType; // 1--------"Only to view",2-------"Can update"
	
	private String orgId; //所属公司
	
	private String orgName;//公司名
	
	private Collection OwnCompanyList = new ArrayList(); //自己所属公司（包括下属公司的集合）
	
	private String upperCompanys ; //所有上级公司的字符串，形如：'0001','0002'(包括自己）
	
	private String lowerCompanys; //所有下级公司的字符串，形如：'0001','0002'(包括自己）
	
	private Collection teamList; //所属公司的所有部门集合
	
	
	public Collection getTeamList() {
		return teamList;
	}

	public void setTeamList(Collection teamList) {
		this.teamList = teamList;
	}

	public String getLowerCompanys() {
		return lowerCompanys;
	}

	public void setLowerCompanys(String lowerCompanys) {
		this.lowerCompanys = lowerCompanys;
	}

	public Collection getOwnCompanyList() {
		return OwnCompanyList;
